{
	"name": "coldtrick/widget_pack",
	"description": "Various Elgg widgets",
	"type": "elgg-plugin",
	"keywords": ["elgg", "plugin", "widgets"],
	"license": "GPL-2.0-only",
	"support": {
		"source": "https://github.com/ColdTrick/widget_pack",
		"issues": "https://github.com/ColdTrick/widget_pack/issues"
	},
	"require": {
		"composer/installers": "^1.0.8",
		"bower-asset/flexslider-customized": "2.6.4",
		"simplepie/simplepie": "~1.5.0"
	},
	"require-dev": {
		"coldtrick/sniffs": "dev-master",
		"coldtrick/releases": "dev-master"
	},
	"conflict": {
		"elgg/elgg": "<3.0"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"config": {
		"fxp-asset": {
			"enabled": false
		}
	}
}
