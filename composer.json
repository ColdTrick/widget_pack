{
	"name": "coldtrick/widget_pack",
	"description": "Various Elgg widgets",
	"homepage": "https://www.coldtrick.com",
	"type": "elgg-plugin",
	"keywords": ["elgg", "plugin", "widget"],
	"license": "GPL-2.0-only",
	"support": {
